{
  "name": "Reaction",
  "version": "1.0.0",
  "main": "index.js",
  "author": "khirayama <https://github.com/khirayama>",
  "license": "MIT",
  "scripts": {
    "start": "npm run development",
    "development": "npm run watch:server & npm run watch:client",
    "build:server": "NODE_PATH=./ NODE_ENV=development babel-node server/app.js",
    "watch:server": "nodemon --ignore client --ignore public --exec npm run build:server",
    "build:client": "NODE_PATH=./ gulp build",
    "watch:client": "NODE_PATH=./ gulp watch",
    "lint": "NODE_PATH=./ eslint",
    "lint:fix": "NODE_PATH=./ eslint --fix"
  },
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "browserify": "^13.1.1",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "cssnano": "^3.8.0",
    "eslint": "^3.10.2",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-config-xo-space": "^0.15.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.2.0",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "plumber": "^0.4.8",
    "postcss-easy-import": "^1.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "source": "^0.0.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
